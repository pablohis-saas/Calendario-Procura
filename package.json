{
  "name": "procura-integrated-system",
  "version": "1.0.0",
  "description": "Sistema integrado de ProCura con mÃ³dulos de cobros e inventario",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:cobros\" \"npm run dev:inventory\" \"npm run dev:frontend\"",
    "dev:cobros": "cd modulosjuntos/backend && npm run dev",
    "dev:inventory": "cd inventory-module && npm run dev",
    "dev:frontend": "cd modulosjuntos/frontend && npm run dev",
    "build": "npm run build:cobros; npm run build:inventory; npm run build:frontend",
    "build:cobros": "cd modulosjuntos/backend && npm run build",
    "build:inventory": "cd inventory-module && npm run build",
    "build:frontend": "cd modulosjuntos/frontend && npm run build",
    "install:all": "npm install && cd modulosjuntos/backend && npm install && cd ../../inventory-module && npm install && cd ../modulosjuntos/frontend && npm install",
    "start": "concurrently \"npm run start:cobros\" \"npm run start:inventory\" \"npm run start:frontend\"",
    "start:cobros": "cd modulosjuntos/backend && npm start",
    "start:inventory": "cd inventory-module && npm start",
    "start:frontend": "cd modulosjuntos/frontend && npm run dev"
  },
  "keywords": [
    "procura",
    "cobros",
    "inventario",
    "integrated",
    "medical",
    "google-calendar",
    "whatsapp"
  ],
  "author": "Rodrigo Espinosa",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.11.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "axios": "^1.11.0",
    "sonner": "^2.0.6"
  }
}
